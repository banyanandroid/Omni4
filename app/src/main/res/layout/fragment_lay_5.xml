            android:id="@+id/frag5_pt1"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:layout_alignBottom="@+id/frag5_bull1"
            android:layout_toRightOf="@+id/frag5_bull1"
            android:layout_toEndOf="@+id/frag5_bull1" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/arrow_1"
            android:layout_marginRight="200dp"
            android:layout_alignTop="@+id/frag5_pt1"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:src="@drawable/arw" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:id="@+id/frag5_bull2"
            android:layout_marginTop="15dp"
            android:layout_below="@+id/frag5_bull1"
            android:layout_alignLeft="@+id/frag5_bull1"
