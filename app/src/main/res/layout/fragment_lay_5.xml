        <ImageView
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:id="@+id/frag5_bull4"
            android:layout_marginTop="15dp"
            android:layout_below="@+id/frag5_bull3"
            android:layout_alignLeft="@+id/frag5_bull3"
            android:layout_alignStart="@+id/frag5_bull3"
            android:src="@drawable/bullet" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Regulates CSC, self-renewal pathways"
            android:id="@+id/frag5_pt4"
            android:textColor="@color/black"
            android:layout_alignTop="@+id/frag5_bull4"
            android:layout_alignLeft="@+id/frag5_pt3"
            android:layout_alignStart="@+id/frag5_pt3" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/arrow_4"
            android:src="@drawable/arw"
            android:layout_alignTop="@+id/frag5_pt4"
            android:layout_alignLeft="@+id/arrow_3"
            android:layout_alignStart="@+id/arrow_3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="05"
            android:textStyle="bold"
            android:layout_marginBottom="10dp"
            android:id="@+id/textView47"
            android:layout_alignParentBottom="true"
            android:layout_alignLeft="@+id/frag5_bull4"
            android:layout_alignStart="@+id/frag5_bull4" />

        <ImageView
            android:layout_width="150dp"
            android:layout_height="60dp"
            android:id="@+id/ultra_img"
            android:visibility="invisible"
            android:layout_marginBottom="75dp"
            android:layout_marginRight="15dp"
            android:src="@drawable/ultra_logo"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />


    </RelativeLayout>
</FrameLayout>