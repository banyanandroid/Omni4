        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/arrow_2"
            android:layout_alignTop="@+id/frag5_pt2_1"
            android:layout_alignRight="@+id/arrow_1"
            android:layout_alignEnd="@+id/arrow_1"
            android:src="@drawable/arw" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="30dp"
            android:id="@+id/imageView17"
            android:src="@drawable/bullet"
            android:layout_marginTop="15dp"
            android:layout_below="@+id/frag5_pt2_1"
            android:layout_alignLeft="@+id/imageView15"
            android:layout_alignStart="@+id/imageView15" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Corrects cachexia, muscle wasting and cancer-related fatigue"
            android:id="@+id/frag5_pt2_2"
            android:textColor="@color/black"
            android:layout_alignBottom="@+id/imageView17"
            android:layout_alignLeft="@+id/frag5_pt2_1"
            android:layout_alignStart="@+id/frag5_pt2_1" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/arrow_2_2"
            android:layout_alignTop="@+id/frag5_pt2_2"
            android:layout_alignLeft="@+id/arrow_2"
            android:layout_alignStart="@+id/arrow_2"
            android:src="@drawable/arw" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="30dp"
            android:id="@+id/imageView19"
            android:layout_marginTop="15dp"
            android:layout_below="@+id/arrow_2_2"
            android:layout_alignLeft="@+id/imageView17"
            android:layout_alignStart="@+id/imageView17"
            android:src="@drawable/bullet" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Restores normalization of mucosal lining"
            android:id="@+id/frag5_pt2_3"
            android:textColor="@color/black"
            android:layout_alignTop="@+id/imageView19"
            android:layout_alignLeft="@+id/frag5_pt2_2"
