<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/conk"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="Fragments.Fragment_11">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <include layout="@layout/back_1"></include>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Curcumin regulates chronic inflammation"
            android:id="@+id/textView71"
            android:textSize="28sp"
            android:textStyle="bold"
            android:textColor="@color/red"
            android:layout_marginLeft="43dp"
            android:layout_marginStart="43dp"
            android:layout_marginTop="84dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:id="@+id/frag22_bul"
            android:layout_marginTop="15dp"
            android:layout_below="@+id/textView71"
            android:layout_alignLeft="@+id/textView71"
            android:layout_alignStart="@+id/textView71"
            android:src="@drawable/bullet" />

        <TextView
            android:layout_width="600dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/frag22"
            android:id="@+id/textView72"
            android:textSize="25sp"
            android:layout_marginLeft="10dp"
            android:layout_alignTop="@+id/frag22_bul"
            android:layout_toRightOf="@+id/frag22_bul"
            android:layout_toEndOf="@+id/frag22_bul" />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:id="@+id/imageView56"
            android:src="@drawable/bullet"
            android:layout_marginTop="15dp"
            android:layout_below="@+id/textView72"
            android:layout_alignLeft="@+id/frag22_bul"
            android:layout_alignStart="@+id/frag22_bul" />

        <TextView
            android:layout_width="600dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/frag22pt2"
            android:id="@+id/textView73"
            android:textSize="25sp"
            android:layout_alignTop="@+id/imageView56"
            android:layout_alignLeft="@+id/textView72"
            android:layout_alignStart="@+id/textView72" />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:id="@+id/imageView57"
            android:src="@drawable/bullet"
            android:layout_marginTop="15dp"
            android:layout_below="@+id/textView73"
            android:layout_alignLeft="@+id/imageView56"
            android:layout_alignStart="@+id/imageView56" />

        <TextView
            android:layout_width="600dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/frag22pt3"
            android:id="@+id/textView74"
            android:textSize="25sp"
            android:layout_alignTop="@+id/imageView57"
            android:layout_alignLeft="@+id/textView73"
            android:layout_alignStart="@+id/textView73" />

    </RelativeLayout>
</FrameLayout>