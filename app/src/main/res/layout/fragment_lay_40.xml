<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/conk"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="Fragments.Fragment_11">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/imageView3"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/omni_bg"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_marginRight="40dp"
                android:src="@drawable/center_2" />

            <ImageView
                android:id="@+id/center_bg"
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/omni" />

            <TextView
                android:id="@+id/strength_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/omni_bg"
                android:layout_alignStart="@+id/omni_bg"
                android:layout_below="@+id/omni_bg"
                android:gravity="center"
                android:text="@string/strength"
                android:textColor="@color/navy"
                android:textSize="14sp"
                android:layout_alignRight="@+id/omni_bg"
                android:layout_alignEnd="@+id/omni_bg" />

            <View
                android:id="@+id/view3"
                android:layout_width="fill_parent"
                android:layout_height="15dip"
                android:layout_above="@+id/on"
                android:background="@color/red" />

            <View
                android:id="@+id/on"
                android:layout_width="fill_parent"
                android:layout_height="27dp"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="3dp"
                android:background="@color/yellow" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/strength_bg"
                android:layout_alignLeft="@+id/r_1"
                android:layout_alignStart="@+id/r_1"
                android:layout_toLeftOf="@+id/omni_bg"
                android:gravity="center"
                android:text="A novel option for controlling Diabetes"
                android:textColor="@color/red"
                android:textSize="34sp"
                android:textStyle="bold" />

            <RelativeLayout
                android:id="@+id/r_1"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_alignLeft="@+id/center_bg"
                android:layout_alignStart="@+id/center_bg"
                android:layout_below="@+id/textView5"
                android:layout_marginTop="15dp">

                <ImageView
                    android:id="@+id/bullet_1"
                    android:layout_width="60dp"
                    android:layout_height="35dp"
                    android:layout_marginLeft="20dp"
                    android:src="@drawable/bullet" />

                <TextView
                    android:id="@+id/pt_1_frag11"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="5dp"
                    android:layout_toRightOf="@+id/bullet_1"
                    android:text="Curcumin exerts significant control over Diabetes at multiple levels and at different stages."
                    android:textColor="@color/black"
                    android:textSize="25sp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/r_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/r_1"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@+id/prev"
                android:layout_toLeftOf="@+id/next">

                <ImageView
                    android:id="@+id/bullet_2"
                    android:layout_width="80dp"
                    android:layout_height="30sp"
                    android:layout_marginLeft="20dp"
                    android:src="@drawable/bullet"
                    android:visibility="invisible" />

                <TextView
                    android:id="@+id/pt_two_frag5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/bullet_2"
                    android:layout_marginLeft="118dp"
                    android:layout_toEndOf="@+id/bullet_2"
                    android:layout_toRightOf="@+id/bullet_2"
                    android:text="Pre - Diabetic"
                    android:textColor="@color/black"
                    android:textSize="21sp" />

                <ImageView
                    android:id="@+id/arrow_2"
                    android:layout_width="30dp"
                    android:layout_height="31dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:clickable="true"
                    android:gravity="center|end"
                    android:src="@drawable/arw"
                    android:textColor="@color/red"
                    android:textSize="30sp"
                    android:textStyle="bold" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/r_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/r_2"
                android:layout_alignLeft="@+id/r_2"
                android:layout_alignRight="@+id/r_2"
                android:layout_alignStart="@+id/r_2"
                android:layout_below="@id/r_2"
                android:layout_marginTop="10dp">

                <ImageView
                    android:id="@+id/bullet_3"
                    android:layout_width="80dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="20dp"
                    android:src="@drawable/bullet"
                    android:visibility="invisible" />

                <TextView
                    android:id="@+id/pt_three_frag5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="118dp"
                    android:layout_toEndOf="@+id/bullet_3"
                    android:layout_toRightOf="@+id/bullet_3"
                    android:gravity="left"
                    android:text="Diabetic"
                    android:textColor="@color/black"
                    android:textSize="21sp" />

                <ImageView
                    android:id="@+id/arrow_3"
                    android:layout_width="30dp"
                    android:layout_height="31dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="20dp"
                    android:clickable="true"
                    android:gravity="center|end"
                    android:src="@drawable/arw"
                    android:textColor="@color/red"
                    android:textStyle="bold" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/r_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/r_3"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignRight="@+id/r_3"
                android:layout_below="@id/r_3"
                android:layout_marginTop="10dp">

                <ImageView
                    android:id="@+id/bullet_4"
                    android:layout_width="80dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="20dp"
                    android:src="@drawable/bullet"
                    android:visibility="invisible" />

                <TextView
                    android:id="@+id/pt_four_frag5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="Diabetic Complications"
                    android:textColor="@color/black"
                    android:textSize="21sp"
                    android:layout_marginRight="140dp"
                    android:layout_marginEnd="140dp"
                    android:layout_alignParentTop="true"
                    android:layout_toLeftOf="@+id/arrow_4"
                    android:layout_toStartOf="@+id/arrow_4" />

                <ImageView
                    android:id="@+id/arrow_4"
                    android:layout_width="30dp"
                    android:layout_height="31dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="28dp"
                    android:clickable="true"
                    android:gravity="end"
                    android:src="@drawable/arw"
                    android:textColor="@color/red"
                    android:textSize="30sp"
                    android:textStyle="bold" />

            </RelativeLayout>


            <ImageView
                android:id="@+id/prev"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/view3"
                android:layout_alignLeft="@+id/r_1"
                android:layout_alignStart="@+id/r_1"
                android:src="@drawable/prev"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/view3"
                android:layout_centerHorizontal="true"
                android:text="Dosage : 2 Tablets daily or as directed by Physician."
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#ff0000" />

            <ImageView
                android:id="@+id/next"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/view3"
                android:layout_alignEnd="@+id/omni_bg"
                android:layout_alignRight="@+id/omni_bg"
                android:layout_alignTop="@+id/prev"
                android:layout_toRightOf="@+id/textView15"
                android:clickable="true"
                android:src="@drawable/next" />


        </RelativeLayout>

        <TextView
            android:id="@+id/textno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"
            android:text="40"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/pt_last_frag11"
            android:layout_alignRight="@+id/pt_last_frag11"
            android:layout_alignTop="@+id/pt_last_frag11" />

    </RelativeLayout>
</FrameLayout>