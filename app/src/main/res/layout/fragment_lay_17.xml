<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/conk"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="Fragments.Fragment_11">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/imageView3"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/omni_bg"
                android:layout_width="150dp"
                android:layout_height="130dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_marginRight="40dp"
                android:src="@drawable/center_2" />

            <ImageView
                android:id="@+id/center_bg"
                android:layout_width="130dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/omni" />

            <TextView
                android:id="@+id/strength_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_alignLeft="@+id/omni_bg"
                android:layout_alignStart="@+id/omni_bg"
                android:layout_below="@+id/omni_bg"
                android:text="@string/strength"
                android:textColor="@color/navy"
                android:textSize="14sp"
                android:layout_alignRight="@+id/omni_bg"
                android:layout_alignEnd="@+id/omni_bg" />

            <View
                android:id="@+id/view3"
                android:layout_width="fill_parent"
                android:layout_height="15dip"
                android:layout_above="@+id/on"
                android:background="@color/red" />

            <View
                android:id="@+id/on"
                android:layout_width="fill_parent"
                android:layout_height="27dp"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="3dp"
                android:background="@color/yellow" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="460dp"
                android:layout_height="70dp"
                android:layout_above="@+id/textView5"
                android:layout_alignEnd="@+id/textView11"
                android:layout_alignParentTop="true"
                android:layout_alignRight="@+id/textView11"
                android:layout_marginTop="10dp"
                android:scaleType="matrix"
                android:src="@drawable/img_18" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="A novel option for maintenance/prevention in IBD"
                android:textColor="@color/red"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_below="@+id/omni_bg"
                android:layout_toRightOf="@+id/prev"
                android:layout_toEndOf="@+id/prev" />

            <RelativeLayout
                android:id="@+id/r_1"
                android:layout_width="match_parent"
                android:layout_height="54dp"
                android:layout_alignLeft="@+id/center_bg"
                android:layout_alignStart="@+id/center_bg"
                android:layout_below="@+id/textView5"
                android:layout_marginTop="10dp">

                <ImageView
                    android:id="@+id/bullet_1"
                    android:layout_width="60dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="20dp"
                    android:src="@drawable/bullet" />

                <TextView
                    android:id="@+id/pt_1_frag11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="20dp"
                    android:layout_toRightOf="@+id/bullet_1"
                    android:text="Inhibits release of NF-Kappa B, including arachidonic acid cascade Modulates production of various inflammatory cytokines"
                    android:textColor="@color/black"
                    android:textSize="20sp"
                    android:textStyle="bold" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/r_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/r_1"
                android:layout_alignStart="@+id/r_1"
                android:layout_below="@+id/r_1"
                android:layout_marginTop="5dp">

                <ImageView
                    android:id="@+id/ico_7"
                    android:layout_width="60dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="20dp"
                    android:src="@drawable/bullet" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignBottom="@+id/imageView7"
                        android:layout_marginLeft="100dp"
                        android:layout_toRightOf="@+id/imageView7"
                        android:text="Treatment for 6 months"
                        android:textColor="@color/black"
                        android:textSize="22sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView16"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignBottom="@+id/imageView7"
                        android:layout_marginLeft="120dp"
                        android:layout_toRightOf="@+id/imageView7"
                        android:text="Reduces clinical activity index and helps maintains remission in U.C."
                        android:textColor="@color/black"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/textView26"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignBottom="@+id/imageView7"
                        android:layout_marginLeft="120dp"
                        android:layout_toRightOf="@+id/imageView7"
                        android:text="Improves survival and colonic morphology"
                        android:textColor="@color/black"
                        android:textSize="20sp" />

                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/r_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/r_2"
                android:layout_alignStart="@+id/r_2"
                android:layout_below="@+id/r_2"
                android:layout_marginTop="5dp">

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="60dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="20dp"
                    android:src="@drawable/bullet" />

                <LinearLayout
                    android:id="@+id/linearLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_alignBottom="@+id/imageView8"
                        android:layout_marginLeft="100dp"
                        android:layout_toRightOf="@+id/imageView8"
                        android:text="Strengthen Intestinal muscles"
                        android:textColor="@color/black"
                        android:textSize="22sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView18"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_alignBottom="@+id/imageView8"
                        android:layout_marginLeft="120dp"
                        android:layout_toRightOf="@+id/imageView8"
                        android:text="Prevents leakage of bacterial to kidney, liver"
                        android:textColor="@color/black"
                        android:textSize="20sp" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="280dp"
                    android:layout_height="40dp"
                    android:layout_below="@+id/linearLayout"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/clinical_16" />
            </RelativeLayout>

            <ImageView
                android:id="@+id/prev"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/view3"
                android:layout_alignLeft="@+id/r_3"
                android:layout_alignStart="@+id/r_3"
                android:src="@drawable/prev" />

            <ImageView
                android:id="@+id/next"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/next"
                android:layout_marginRight="20dp"
                android:layout_above="@+id/view3"
                android:layout_alignRight="@+id/r_3"
                android:layout_alignEnd="@+id/r_3" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/r_3"
                android:layout_centerHorizontal="true"
                android:text="Dosage : 2 Tablets daily or as directed by Physician."
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#ff0000" />

        </RelativeLayout>


        <TextView
            android:id="@+id/textno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"
            android:text="17"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/pt_last_frag11"
            android:layout_alignRight="@+id/pt_last_frag11"
            android:layout_alignTop="@+id/pt_last_frag11" />

    </RelativeLayout>
</FrameLayout>